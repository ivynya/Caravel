
{
  "canvas": {
    "domain": {
      "name": "App Domain",
      "type": "text",
      "default": "foothillcollege.instructure.com",
      "value": "foothillcollege.instructure.com"
    },
    "proxy": {
      "name": "Content Proxy",
      "type": "text",
      "default": "cors.sdbagel.com",
      "value": "cors.sdbagel.com"
    }
  },
  "caravel": {
    "accent": {
      "name": "Accent Color",
      "type": "select",
      "default": "ibm-blue",
      "value": "ibm-blue",
      "options": [
        "ibm-blue",
        "vyne-green",
        "vyne-teal"
      ]
    },
    "theme": {
      "name": "Theme",
      "type": "select",
      "default": "caravel-light",
      "value": "caravel-light",
      "options": [
        "system",
        "caravel-dark",
        "caravel-light",
        "soulless-dark",
        "soulless-light"
      ]
    }
  },
  "course": {
    "show_breadcrumbs": {
      "name": "Show Breadcrumbs",
      "type": "toggle",
      "default": true,
      "value": true
    },
    "use_redesign": {
      "name": "Use Redesigned Home",
      "type": "toggle",
      "default": true,
      "value": true
    }
  },
  "networking": {
    "stop_calls_if_cached": {
      "name": "Low Bandwidth Mode",
      "type": "toggle",
      "default": false,
      "value": false
    },
    "offline_mode": {
      "name": "Freeze App Data",
      "type": "toggle",
      "default": false,
      "value": false
    }
  },
  "notifications": {
    "compact_banners": {
      "name": "Use Compact Banners",
      "type": "toggle",
      "default": false,
      "value": false
    },
    "extend_critical_alerts": {
      "name": "Show Critical Alerts for Longer",
      "type": "toggle",
      "default": true,
      "value": true
    }
  }
}