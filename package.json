{
  "name": "caravel",
  "version": "0.5.6",
  "description": "Modern, fast, and performant LMS client.",
  "homepage": "https://github.com/SDBagel/Caravan",
  "author": {
    "name": "SDBagel",
    "email": "sdbagel@gmail.com"
  },
  "keywords": [
    "angular",
    "electron",
    "nodejs",
    "typescript",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux",
    "caravan",
    "caravel"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href /",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o --host 0.0.0.0",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.2.0",
    "@angular-devkit/build-angular": "12.2.1",
    "@angular-eslint/builder": "12.2.1",
    "@angular-eslint/eslint-plugin": "12.2.1",
    "@angular-eslint/eslint-plugin-template": "12.2.1",
    "@angular-eslint/template-parser": "12.2.1",
    "@angular/cli": "12.2.1",
    "@angular/common": "12.2.1",
    "@angular/compiler": "12.2.1",
    "@angular/compiler-cli": "12.2.1",
    "@angular/core": "12.2.1",
    "@angular/forms": "12.2.1",
    "@angular/language-service": "12.2.1",
    "@angular/platform-browser": "12.2.1",
    "@angular/platform-browser-dynamic": "12.2.1",
    "@angular/router": "12.2.1",
    "@angular/service-worker": "12.2.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.0.34",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "electron": "13.3.0",
    "electron-builder": "22.8.0",
    "electron-reload": "1.5.0",
    "eslint": "7.6.0",
    "eslint-plugin-import": "2.22.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "6.3.16",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "8.1.1",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.2",
    "spectron": "11.1.0",
    "ts-node": "8.10.2",
    "tslib": "^2.0.0",
    "typescript": "4.3.5",
    "wait-on": "5.2.1",
    "webdriver-manager": "12.1.7",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": "16.3.x"
  },
  "dependencies": {
    "angular2-qrcode": "2.0.3"
  }
}